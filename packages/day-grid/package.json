{
	"name": "@event-calendar/day-grid",
	"version": "0.18.1",
	"title": "Event Calendar DayGrid plugin",
	"description": "Full-sized drag & drop event calendar with resource view",
	"keywords": [
		"calendar",
		"event",
		"resource",
		"full-sized"
	],
	"homepage": "https://vkurko.github.io/calendar/",
	"repository": {
		"type": "git",
		"url": "https://github.com/vkurko/calendar.git",
		"directory": "packages/day-grid"
	},
	"funding": [
		{
			"type": "paypal_$10",
			"url": "https://paypal.me/vkurko/10usd"
		},
		{
			"type": "paypal_$1",
			"url": "https://paypal.me/vkurko/1usd"
		}
	],
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm run package",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore && eslint .",
		"format": "prettier --write . --config ../../.prettierrc --ignore-path .gitignore"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.58.0"
	},
	"dependencies": {
		"@event-calendar/common": "workspace:~"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "4.3.3",
		"vitest": "^0.30.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
